    <result column="auth_code" jdbcType="VARCHAR" property="authCode" />
    <result column="suite_key" jdbcType="VARCHAR" property="suiteKey" />
    <result column="time_stamp" jdbcType="VARCHAR" property="timeStamp" />
  </resultMap>

  <select id="getAuthCodeByCorpId" parameterType="java.lang.String" resultType="java.lang.String">
    select auth_code
    from callback_info
    where auth_corp_id = #{corpId}
  </select>

  <insert id="insert" parameterType="com.learning.cloud.index.entity.CallbackInfo">
    insert into callback_info
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
