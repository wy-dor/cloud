        dimension_name,
      </if>
      <if test="iconId != null">
        icon_id,
      </if>
      <if test="schoolId != null">
        school_id,
      </if>
      <if test="updateUserId != null">
        update_user_id,
      </if>
      <if test="updateUserName != null">
        update_user_name,
      </if>
      <if test="builtin != null">
        builtin,
      </if>
