<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.learning.cloud.evaluation.dao.EvaluationDimensionDao">

  <select id="getById" parameterType="java.lang.Long" resultType="com.learning.cloud.evaluation.entity.EvaluationDimension">
    select *
    from evaluation_dimension
    where id = #{id}
  </select>
  
